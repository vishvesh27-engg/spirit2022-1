                                        break;

                                    }
                                    case 'fitness': {
                                        img_src = 'images/Fitness.png'; 
                                        img_name = 'Fitness';
                                        break;
                                    }
                                    case 'fantasy': {
                                        img_src = 'images/CricketFantasy.jpg'; 
                                        img_name = 'Fantasy';
                                        break;
                                    }
                                    case 'treasure_hunt': {
                                        img_src = 'images/Treasure.jpg'; 
                                        img_name = 'Treasure Hunt';
                                        break;
                                    }
                                    case 'cricket_workshop': {
                                        img_src = 'images/Cricketworkshop.jpg'; 
                                        img_name = 'Cricketworkshop';
                                        break;
                                    }
                                    case 'chess': {
                                        img_src = 'images/Chess.jpeg'; 
                                        img_name = 'Chess';
                                        break;
                                    }
                                    case 'Ipl Auction': 
                                        if(isIpladded == false){
                                            img_src = 'images/Ipl.png';
                                            img_name = 'IPL Auction'
                                        } 
                                        break;
                                    }
                                if(img_src != ''){ %>
